<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:name="com.google.android.gms.maps.MapFragment"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    >
    <!--tools:context=".NaviActivity" 를 일단 생략. ppt엔 삽입되어있음.!-->

    <ListView
        android:id = "@+id/selectExerciseListView"
        android:layout_width="match_parent"
        android:layout_height="500dp" >
    </ListView>
    <Button
        android:layout_width="match_parent"
        android:layout_height="50dp"
        android:id="@+id/selectExerciseButton"
        android:text="운동 시작하기"
        android:fontFamily="@font/freesentation_6_semibold"
        android:textSize="22sp"
        android:backgroundTint="#0047BB"
        android:layout_gravity="bottom|center"
        android:visibility="visible"
        android:layout_marginBottom="150dp"
        android:layout_marginRight="60dp"
        android:layout_marginLeft="60dp"
        />
</FrameLayout>
